PREFIX : <https://linked.data.gov.au/def/gn-part-types/>
PREFIX cs: <https://linked.data.gov.au/def/gn-part-types>
PREFIX agldwgstatus: <https://linked.data.gov.au/def/reg-statuses/>
PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX isoroles: <http://def.isotc211.org/iso19115/-1/2018/CitationAndResponsiblePartyInformation/code/CI_RoleCode/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX reg: <http://purl.org/linked-data/registry#>
PREFIX sdo: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:geographicalPrefix
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "A prefix to the main geographical name parts"@en ;
    skos:historyNote "Created for this vocabulary" ;
    skos:inScheme cs: ;
    skos:prefLabel "Geographical Prefix"@en ;
    skos:topConceptOf cs: ;
.

:geographicalGivenName
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "A name given specifically to the geographical object"@en ;
    skos:historyNote "Created for this vocabulary" ;
    skos:inScheme cs: ;
    skos:prefLabel "Geographical Given Name"@en ;
    skos:topConceptOf cs: ;
.

:geographicalSuffix
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:definition "A suffix to the main geographical name parts"@en ;
    skos:historyNote "Created for this vocabulary" ;
    skos:inScheme cs: ;
    skos:prefLabel "Geographical Suffix"@en ;
    skos:topConceptOf cs: ;
.

<https://linked.data.gov.au/org/icsm>
    a sdo:Organization ;
    sdo:name "Intergovernmental Committee on Surveying & Mapping" ;
    sdo:url "https://icsm.gov.au"^^xsd:anyURI ;
.

<https://linked.data.gov.au/org/qsi>
    a sdo:Organization ;
    sdo:name "Queensland Spatial Information" ;
    sdo:url "https://www.resources.qld.gov.au/data-mapping"^^xsd:anyURI ;
.

cs:
    a skos:ConceptScheme ;
    dcterms:created "2023-06-12"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/qsi> ;
    dcterms:modified "2023-11-30"^^xsd:date ;
    dcterms:publisher <https://linked.data.gov.au/org/icsm> ;
    owl:versionIRI :1.0 ;
    owl:versionInfo "1.0" ;
    skos:definition "This vocabulary describes values of name part types used in geographical naming"@en ;
    skos:hasTopConcept
        :geographicalPrefix ,
        :geographicalGivenName ,
        :geographicalSuffix ;
    skos:historyNote "This vocabulary contains additional types (subclasses) of Compound Names used for geographical objects"@en ;
    skos:prefLabel "Geographical Name Part Types"@en ;
    reg:status agldwgstatus:experimental ;
    prov:qualifiedAttribution [
            dcat:hadRole isoroles:custodian ;
            prov:agent <https://linked.data.gov.au/org/icsm>
        ] ;
.

