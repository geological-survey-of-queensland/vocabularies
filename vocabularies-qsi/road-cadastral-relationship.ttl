PREFIX : <https://linked.data.gov.au/def/road-cadastral-relationship/>
PREFIX agldwgstatus: <https://linked.data.gov.au/def/reg-statuses/>
PREFIX cs: <https://linked.data.gov.au/def/road-cadastral-relationship>
PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX isoroles: <http://def.isotc211.org/iso19115/-1/2018/CitationAndResponsiblePartyInformation/code/CI_RoleCode/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX reg: <http://purl.org/linked-data/registry#>
PREFIX sdo: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX themes: <https://linked.data.gov.au/def/fsdf/themes/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:not-related
    a skos:Concept ;
    skos:definition "Road segment is not related to an area of road in the cadastre"@en ;
    skos:historyNote "Queensland Roads and Tracks dataset" ;
    skos:inScheme cs: ;
    skos:prefLabel "No Relationship To Cadastral Road Area"@en ;
.

:related-off
    a skos:Concept ;
    skos:definition "Road segment is related to an area of road in the cadastre, but part of the centreline lies outside of this area"@en ;
    skos:historyNote "Queensland Roads and Tracks dataset" ;
    skos:inScheme cs: ;
    skos:prefLabel "Off Alignment To Cadastral Road Area"@en ;
.

:related-within
    a skos:Concept ;
    skos:definition "Road segment is related to an area of road in the cadastre, and the centreline lies inside of this area"@en ;
    skos:historyNote "Queensland Roads and Tracks dataset" ;
    skos:inScheme cs: ;
    skos:prefLabel "Within Cadastral Road Area"@en ;
.

:unknown
    a skos:Concept ;
    skos:altLabel
        "U"@en ,
        "Unknown"@en ;
    skos:definition "Unknown if the road segment is related to an area of road in the cadastre"@en ;
    skos:historyNote "Queensland Roads and Tracks dataset" ;
    skos:inScheme cs: ;
    skos:prefLabel "Unknown Relationship To Cadastral Road Area"@en ;
.

<https://linked.data.gov.au/org/qsi>
    a sdo:Organization ;
    sdo:name "Queensland Spatial Information" ;
    sdo:url "https://www.resources.qld.gov.au/data-mapping"^^xsd:anyURI ;
.

<https://linked.data.gov.au/org/icsm>
    a sdo:Organization ;
    sdo:name "Intergovernmental Committee on Surveying & Mapping" ;
    sdo:url "https://icsm.gov.au"^^xsd:anyURI ;
.

cs:
    a skos:ConceptScheme ;
    dcat:theme themes:transport ;
    dcterms:created "2023-05-31"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/qsi> ;
    dcterms:identifier "road-cadastral-relationship"^^xsd:token ;
    dcterms:modified "2023-11-30"^^xsd:date ;
    dcterms:publisher <https://linked.data.gov.au/org/icsm> ;
    reg:status agldwgstatus:experimental ;
    owl:versionIRI :1.0 ;
    owl:versionInfo "1.0" ;
    skos:definition "This vocabulary describes the relationship, or alignment, of a road to a cadastral road area"@en ;
    skos:hasTopConcept
        :not-related ,
        :related-off ,
        :related-within ,
        :unknown ;
    skos:historyNote "This vocabulary was created by the Queensland Spatial Information services." ;
    skos:prefLabel "Road Cadastral Relationship"@en ;
    prov:qualifiedAttribution
        [
            dcat:hadRole isoroles:custodian ;
            prov:agent <https://linked.data.gov.au/org/icsm>
        ] ;
.

