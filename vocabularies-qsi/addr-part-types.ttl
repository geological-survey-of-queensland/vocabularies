cs:
    a skos:ConceptScheme ;
    sdo:keywords themes:place-names ;
    dcterms:created "2023-05-25"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/qsi> ;
    dcterms:modified "2024-06-26"^^xsd:date ;
    dcterms:publisher <https://linked.data.gov.au/org/icsm> ;
    reg:status agldwgstatus:experimental ;
    owl:versionIRI :1.0 ;
    owl:versionInfo "1.0" ;
    skos:definition "This vocabulary describes types of name part types used in addressing"@en ;
    skos:hasTopConcept
        :geographicalObject ,
        :subaddressType ,
        :subaddressNumber ,
        :subaddressNumberPrefix ,
        :subaddressNumberSuffix ,
        :buildingLevelType ,
        :buildingLevelNumber ,
        :buildingLevelNumberPrefix ,
        :buildingLevelNumberSuffix ,
        :streetNumberFirst ,
        :streetNumberFirstPrefix ,
        :streetNumberFirstSuffix ,
        :streetNumberLast ,
        :streetNumberLastPrefix ,
        :streetNumberLastSuffix ,
        :street ,
        :waterFeature ,
        :locality ,
        :stateOrTerritory ,
        :indigenousCountryName ,
        :postcode ,
        :country ;
    skos:historyNote "This vocabulary replaces Address Component Types in ISO19160-1. It is also derived from elements within the Austrlaian Geocoded National Address File (GNAF)."@en ;
    skos:prefLabel "Address Name Part Types"@en ;
    prov:qualifiedAttribution
        [
            prov:hadRole droles:custodian ;
            prov:agent <https://linked.data.gov.au/org/icsm>
        ] ;
.

