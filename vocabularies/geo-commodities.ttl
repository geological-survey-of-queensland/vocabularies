    skos:definition "Commodity is a chemical compound that is extracted/processed from an ore material mined from the Earth."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/chemical-compound-product> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Chemical Compound Product"@en .

cdt:chemical-oxide-product a skos:Concept ;
    skos:broader cdt:chemical-compound-product ;
    skos:definition "Commodity is a chemical compound product oxide that is extracted/processed from an ore material mined from the Earth."@en ;
