    skos:prefLabel "Chemical Oxide Product"@en .

cdt:alumina a skos:Concept ;
    skos:altLabel "aluminium oxide"@en,
        "aluminum oxide"@en ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "Alumina is a fairly chemically inert and white chemical compound of aluminium and oxygen with the chemical formula Al2O3. It is the most commonly occurring of several aluminium oxides, and specifically identified as aluminium(III) oxide. It may also be called aloxide, aloxite, or alundum depending on particular forms or applications. Used as filler in plastic and cosmetics, as a catalyst."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/alumina> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Alumina"@en .

cdt:chrome a skos:Concept ;
    skos:altLabel "chrome"@en ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "A green-coloured oxide of chromium, used as a pigment, originally called viridian."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/chrome> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Chromium Oxide"@en .

cdt:iron-oxide a skos:Concept ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "Commodity of interest is any of the sixteen known iron oxides and oxyhydroxides, extracted from the mined material, exclusive of hematite or magnetite produced as an industrial mineral."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/iron-oxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Iron Oxide"@en .

cdt:magnetite a skos:Concept ;
    skos:altLabel "lodestone"@en ;
    skos:broader cdt:industrial-mineral,
        cdt:iron-oxide ;
    skos:definition "Magnetite is an oxide of iron (as is hematite). It is not a component of ordinary rust, although it can form as iron oxidizes in a dry environment."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/magnetite> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:notation "MT" ;
    skos:prefLabel "Magnetite"@en .

cdt:ochre a skos:Concept ;
    skos:broader cdt:iron-oxide ;
    skos:definition "Ochre is a natural earth pigment containing hydrated iron oxide, which ranges in colour from yellow to deep orange or brown. It is also the name of the colours produced by this pigment, especially a light brownish-yellow. A variant of ochre containing a large amount of hematite, or dehydrated iron oxide, has a reddish tint known as 'red ochre'."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/ochre> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Ochre"@en .

cdt:lithium-oxide a skos:Concept ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "Lithium oxide is used as a flux in ceramic glazes; and creates blues with copper and pinks with cobalt. Lithium oxide reacts with water and steam, forming lithium hydroxide and should be isolated from them."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/lithium-oxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Lithium Oxide"@en .

cdt:magnesia a skos:Concept ;
    skos:altLabel "magnesium oxide"@en ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "Magnesium oxide (MgO), or magnesia, is a white hygroscopic solid mineral that occurs naturally as periclase and is a source of magnesium."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/magnesia> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Magnesia"@en .

cdt:niobium-pentoxide a skos:Concept ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "Niobium pentoxide is the inorganic compound with the formula Nb2O5. It is a colourless insoluble solid that is fairly unreactive. It is the main precursor to all materials made of niobium, the dominant application being alloys, but other specialized applications include capacitors, lithium niobate, and optical glasses."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/niobium-pentoxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Niobium Pentoxide"@en .

cdt:phosphorous-pentoxide a skos:Concept ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "Phosphorus pentoxide is a chemical compound with molecular formula P4O10 (with its common name derived from its empirical formula, P2O5). This white crystalline solid is the anhydride of phosphoric acid. It is a powerful desiccant and dehydrating agent."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/phosphorous-pentoxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Phosphorous Pentoxide"@en .

cdt:rare-earth-oxide a skos:Concept ;
    skos:altLabel "TREO"@en ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "Oxides of rare earth elements (REE) or rare earth metals, rare earth elements or metals being one of a set of seventeen chemical elements in the periodic table, specifically the fifteen lanthanides plus scandium and yttrium. Scandium and yttrium are considered rare earth elements because they tend to occur in the same ore deposits as the lanthanides and exhibit similar chemical properties."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/rare-earth-oxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Rare Earth Oxide"@en .

cdt:heavy-rare-earth-oxide a skos:Concept ;
    skos:altLabel "HREO"@en ;
    skos:broader cdt:rare-earth-oxide ;
    skos:definition "Oxides of rare earth elements with atomic numbers greater than or equal to 62."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/heavy-rare-earth-oxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Heavy Rare Earth Oxide"@en .

cdt:light-rare-earth-oxide a skos:Concept ;
    skos:altLabel "LREO"@en ;
    skos:broader cdt:rare-earth-oxide ;
    skos:definition "Oxides of rare earth elements with atomic numbers 57 to 61, inclusive."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/light-rare-earth-oxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Light Rare Earth Oxide"@en .

cdt:tantalum-pentoxide a skos:Concept ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "An oxide of tantalum also known as tantalum(V) oxide, a white, inert solid with a high refractive index and low absorption (i.e. colourless), useful for coatings, and used in the production of capacitors, due to its high dielectric constant."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/tantalum-pentoxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Tantalum Pentoxide"@en .

cdt:uranium-oxide a skos:Concept ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "Uranium dioxide or uranium(IV) oxide (UO2), also known as urania or uranous oxide, is an oxide of uranium, and is a black, radioactive, crystalline powder that naturally occurs in the mineral uraninite. It is used in nuclear fuel rods in nuclear reactors."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/uranium-oxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Uranium Oxide"@en .

cdt:vanadium-pentoxide a skos:Concept ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "A brown/yellow solid, although when freshly precipitated from aqueous solution, its colour is deep orange. Also known as vanadium(V) oxide or vanadia. Because of its high oxidation state, is both an amphoteric oxide and an oxidising agent, used as a precursor to alloys of vanadium and as an industrial catalyst."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/vanadium-pentoxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Vanadium Pentoxide"@en .

cdt:yttrium-oxide a skos:Concept ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "Yttrium oxide, also known as yttria is an air-stable, white solid substance. Used to make Eu:YVO4 and Eu:Y2O3 phosphors that give the red colour in colour TV picture tubes, to make yttrium iron garnets used in microwave filters, and to make the high temperature superconductor YBa2Cu3O7."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/yttrium-oxide> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Yttrium Oxide"@en .

cdt:zirconia a skos:Concept ;
    skos:altLabel "zirconium dioxide"@en ;
    skos:broader cdt:chemical-oxide-product ;
    skos:definition "A white crystalline oxide of zirconium. Used in the production of ceramics, as a protective coating on particles of titanium dioxide pigments, as a refractory material, in insulation, abrasives and enamels, in oxygen sensors and fuel cell membranes, and as the solid electrolyte in electrochromic devices."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/zirconia> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Zirconia"@en .

cdt:lime a skos:Concept ;
    skos:altLabel "calcium hydroxide"@en,
        "slaked lime"@en ;
    skos:broader cdt:chemical-compound-product ;
    skos:definition "Lime is calcium oxide or calcium hydroxide, but the category is commonly applied to calcium-containing inorganic material in which carbonates, oxides and hydroxides predominate."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/lime> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:notation "LIME" ;
    skos:prefLabel "Lime"@en .

cdt:nitrate a skos:Concept ;
    skos:broader cdt:chemical-compound-product ;
    skos:definition "Nitrates are mainly produced for use as fertilizers in agriculture because of their high solubility and biodegradability. The main nitrates are ammonium, sodium, potassium, and calcium salts. Several million kilograms are produced annually for this purpose."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/nitrate> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Nitrate"@en .

cdt:potash a skos:Concept ;
    skos:broader cdt:chemical-compound-product ;
    skos:definition "Potash is any of various salts that contain potassium in water-soluble form, the most common being potassium chloride (KCl). Mostly used in fertilizers."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/potash> ;
    skos:inScheme <http://linked.data.gov.au/def/geo-commodities> ;
    skos:prefLabel "Potash"@en .

cdt:soda-ash a skos:Concept ;
    skos:altLabel "sodium carbonate"@en,
