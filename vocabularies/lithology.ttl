    skos:definition "An obsolete general term applied to all acid rocks."@en ;
    skos:inScheme <http://linked.data.gov.au/def/lithological-observations> ;
    skos:notation "AVOL" ;
    skos:prefLabel "Acidite"@en .

lith:actinolite-chlorite-schist a skos:Concept ;
    skos:broader lith:greenschist,
        lith:schist ;
    skos:definition "A schistose metamorphic rock containing actinolite and chlorite."@en ;
    skos:inScheme <http://linked.data.gov.au/def/lithological-observations> ;
    skos:notation "ATSH" ;
    skos:prefLabel "Actinolite-chlorite schist"@en .

lith:actinolite-mica-schist a skos:Concept ;
