    skos:notation "AFTC" ;
    skos:prefLabel "Alkaliplete"@en .

lith:alkaliptoche a skos:Concept ;
    skos:broader lith:normal-crystalline-igneous-rock ;
    skos:definition "An obsolete term for igneous rocks poor in alkalis."@en ;
    skos:inScheme <http://linked.data.gov.au/def/lithological-observations> ;
    skos:notation "AKPL" ;
    skos:prefLabel "Alkaliptoche"@en .

lith:alkorthosite a skos:Concept ;
    skos:broader lith:syenite ;
    skos:definition "An obsolete name for a variety of consisting predominantly of Na-orthoclase."@en ;
    skos:inScheme <http://linked.data.gov.au/def/lithological-observations> ;
    skos:notation "AKTS" ;
    skos:prefLabel "Alkorthosite"@en .

lith:allalinite a skos:Concept ;
