    skos:inScheme <http://linked.data.gov.au/def/report-detail-type> ;
    skos:prefLabel "Comments"@en ;
    skos:topConceptOf <http://linked.data.gov.au/def/report-detail-type> .

rptdt:contact a skos:Concept ;
