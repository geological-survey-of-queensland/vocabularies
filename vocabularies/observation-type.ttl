#    skos:inScheme <http://linked.data.gov.au/def/observation-type> ;
#    skos:notation "my" ;
#    skos:prefLabel "Magnetic gradiometry"@en .

obst:seismic a skos:Concept ;
    skos:definition "A observation using the interaction of seismic waves with sub-surface features of the earth to determine properties and morphology of those features."@en ;
    skos:inScheme <http://linked.data.gov.au/def/observation-type> ;
    skos:notation "ss" ;
    skos:prefLabel "Seismic"@en ;
    skos:topConceptOf <http://linked.data.gov.au/def/observation-type> .

obst:electromagnetic a skos:Concept ;
    skos:altLabel "EM"@en,
        "EM Observation"@en ;
    skos:definition "A observation measuring induced electromagnetic fields from the earth to determine the resistive/conductive structure of the earth."@en ;
    skos:inScheme <http://linked.data.gov.au/def/observation-type> ;
    skos:notation "em" ;
    skos:prefLabel "Electromagnetic"@en ;
    skos:topConceptOf <http://linked.data.gov.au/def/observation-type> .

obst:geochemistry a skos:Concept ;
    skos:definition "A observation where the chemical properties of rocks, soil, or other geological material is assessed to determine mineralogical and/or elemental composition."@en ;
    skos:inScheme <http://linked.data.gov.au/def/observation-type> ;
    skos:notation "gc" ;
    skos:prefLabel "Geochemistry"@en ;
    skos:topConceptOf <http://linked.data.gov.au/def/observation-type> .

obst:photographic a skos:Concept ;
    skos:definition "A observation using standard photographic equipment for image capture without intention for spectral analysis. Including land-based geological photography."@en ;
    skos:inScheme <http://linked.data.gov.au/def/observation-type> ;
    skos:notation "ph" ;
    skos:prefLabel "Photographic"@en ;
    skos:topConceptOf <http://linked.data.gov.au/def/observation-type> .

obst:radiometric a skos:Concept ;
    skos:altLabel "Gamma Logs"@en,
        "Gamma-Ray Spectrography"@en ;
    skos:definition "A observation where the emissions of natural gamma-rays are recorded to determine the quantity of radioelements (Potassium, Uranium, Thorium)."@en ;
    skos:inScheme <http://linked.data.gov.au/def/observation-type> ;
    skos:notation "rm" ;
    skos:prefLabel "Radiometric"@en ;
    skos:topConceptOf <http://linked.data.gov.au/def/observation-type> .

obst:spectral a skos:Concept ;
    skos:altLabel "hyperspectral"@en ;
    skos:definition "A observation capturing image information and intensity-per-wavelength-band to be used for spectral analysis e.g. ASTER, MODIS."@en ;
    skos:inScheme <http://linked.data.gov.au/def/observation-type> ;
    skos:notation "sp" ;
    skos:prefLabel "Spectral"@en ;
    skos:topConceptOf <http://linked.data.gov.au/def/observation-type> .

obst:spatial a skos:Concept ;
    skos:definition "A observation measuring the spatial characteristics of an object or region."@en ;
    skos:inScheme <http://linked.data.gov.au/def/observation-type> ;
    skos:notation "st" ;
    skos:prefLabel "Spatial"@en ;
    skos:topConceptOf <http://linked.data.gov.au/def/observation-type> .

obst:gravity a skos:Concept ;
    skos:definition "A observation where the variation of the Earth's gravity field in measured to determine the distribution of density in the region"@en ;
    skos:inScheme <http://linked.data.gov.au/def/observation-type> ;
    skos:notation "gv" ;
    skos:prefLabel "Gravity"@en ;
    skos:topConceptOf <http://linked.data.gov.au/def/observation-type> .

obst:magnetic a skos:Concept ;
    skos:altLabel "aeromagnetics"@en ;
    skos:definition "A observation where the variations of the Earth's magnetic field is measured to determine the distribution of magnetic minerals (predominately magnetite)"@en ;
    skos:inScheme <http://linked.data.gov.au/def/observation-type> ;
    skos:notation "mg" ;
    skos:prefLabel "Magnetic"@en ;
    skos:topConceptOf <http://linked.data.gov.au/def/observation-type> .
