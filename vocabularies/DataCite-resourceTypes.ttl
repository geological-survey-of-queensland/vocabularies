rtype:Event a skos:Concept ;
        skos:prefLabel "Event"@en ;
    rdfs:isDefinedBy <https://linked.data.gov.au/def/resource-types> ;
        skos:inScheme <https://linked.data.gov.au/def/resource-types> ;
        skos:topConceptOf <https://linked.data.gov.au/def/resource-types> ;
        skos:exactMatch <http://registry.it.csiro.au/def/datacite/resourceType/Event> ;
        skos:definition "A non-persistent, time-based occurrence."@en ;
        dcterms:source <https://schema.datacite.org/meta/kernel-4.1/doc/DataCite-MetadataKernel_v4.1.pdf> ;
        skos:notation "Event"@en ;
        skos:example <https://data.datacite.org/application/vnd.datacite.datacite+xml/10.7269/P3RN35SZ> ;
        skos:scopeNote "Descriptive information and/or content that is the basis for discovery of the purpose, location, duration, and responsible agents associated with an event such as a webcast or convention. Ex: https://data.datacite.org/application/vnd.datacite.datacite+xml/10.7269/P3RN35SZ"@en .

rtype:Collection a skos:Concept ;
        skos:prefLabel "Collection"@en ;
    rdfs:isDefinedBy <https://linked.data.gov.au/def/resource-types> ;
        skos:inScheme <https://linked.data.gov.au/def/resource-types> ;
        skos:topConceptOf <https://linked.data.gov.au/def/resource-types> ;
        skos:exactMatch <http://registry.it.csiro.au/def/datacite/resourceType/Collection> ;
        skos:definition  "An aggregation of resources, which may encompass collections of one resourceType as well as those of mixed types. A collection is described as a group; its parts may also be separately described."@en ;
        dcterms:source <https://schema.datacite.org/meta/kernel-4.1/doc/DataCite-MetadataKernel_v4.1.pdf> ;
        skos:notation "Collection"@en ;
        skos:example <https://data.datacite.org/application/vnd.datacite.datacite+xml/10.5284/1001038> ;
        skos:scopeNote "A collection of samples, or various files making up a report. Ex: https://data.datacite.org/application/vnd.datacite.datacite+xml/10.5284/1001038"@en .

rtype:InteractiveResource a skos:Concept ;
        skos:prefLabel "Interactive Resource"@en ;
    rdfs:isDefinedBy <https://linked.data.gov.au/def/resource-types> ;
        skos:inScheme <https://linked.data.gov.au/def/resource-types> ;
        skos:topConceptOf <https://linked.data.gov.au/def/resource-types> ;
        skos:exactMatch <http://registry.it.csiro.au/def/datacite/resourceType/InteractiveResource> ;
