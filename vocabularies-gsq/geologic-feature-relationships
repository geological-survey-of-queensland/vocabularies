    a skos:Concept ;
    skos:historyNote "Compiled by the Geological Survey of Queensland" ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Where the relationship between two geological features is unknown."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Unknown"@en ;
