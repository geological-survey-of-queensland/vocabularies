retstat:disposed
    a skos:Concept ;
    skos:historyNote "Created from the WIMS MIRO brainstorming session" ;
    rdfs:isDefinedBy cs: ;
    skos:definition "This sample has been disposed of and no longer exists in the collection. Cannot be viewed, sampled or disposed."@en ;
    skos:inScheme cs: ;
    skos:notation "disposed" ;
    skos:prefLabel "Disposed"@en ;
    skos:topConceptOf cs: ;
.

retstat:permanent-collection
    a skos:Concept ;
    skos:historyNote "Created from the WIMS MIRO brainstorming session" ;
    rdfs:isDefinedBy cs: ;
    skos:definition "To be held indefinitely, or until a director approves its disposal. Can be viewed and sampled, but not disposed."@en ;
    skos:inScheme cs: ;
    skos:notation "permanent-collection" ;
    skos:prefLabel "Permanent Collection"@en ;
    skos:topConceptOf cs: ;
.

retstat:scientifically-significant 
    a skos:Concept ;
    skos:historyNote "Created from the WIMS MIRO brainstorming session" ;
    rdfs:isDefinedBy cs: ;
    skos:definition "A sample which has been identified as having a scientifically significant section contained within. To be held indefinitely, or until a director approves its disposal. Can be viewed and sampled, but not disposed."@en ;
    skos:inScheme cs: ;
    skos:notation "scientifically-significant " ;
    skos:prefLabel "Scientifically Significant "@en ;
    skos:topConceptOf cs: ;
.

